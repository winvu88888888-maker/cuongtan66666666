            "parameters": {
                "command": "python -c \"{{ $json.python_script }}\"",
                "cwd": "c:/Users/GHC/Desktop/python1 - Copy"
            },
            "id": "execute-2",
            "name": "Tạo Code",
            "type": "n8n-nodes-base.executeCommand",
            "typeVersion": 1,
            "position": [
                650,
                300
            ]
        },
        {
            "parameters": {
                "jsCode": "const output = $input.item.json.stdout || '{}';\nconst result = JSON.parse(output);\n\nreturn [{\n  json: {\n    success: true,\n    code: result.code,\n    validation: result.validation,\n    name: result.name,\n    language: result.language\n  }\n}];"
            },
            "id": "code-4",
