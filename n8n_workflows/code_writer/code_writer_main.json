        {
            "parameters": {
                "jsCode": "const spec = $input.item.json.body || {};\n\nconst pythonScript = `\nimport sys\nimport json\nsys.path.append('c:/Users/GHC/Desktop/python1 - Copy/ai_modules')\nfrom code_writer_ai import CodeWriterAI\n\nwriter = CodeWriterAI()\nspec = ${JSON.stringify(spec)}\nresult = writer.write_code_from_spec(spec)\nprint(json.dumps(result, ensure_ascii=False))\n`;\n\nreturn [{\n  json: {\n    spec: spec,\n    python_script: pythonScript\n  }\n}];"
            },
            "id": "code-3",
            "name": "Chuẩn Bị Spec",
